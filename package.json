{
  "name": "vue-webpack-learning",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clear": "rimraf public && rimraf server-build",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.config.client.js",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.config.server.js",
    "build": "npm run clear && npm run build:client && npm run build:server",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.client.js",
    "dev:server": "nodemon server/server.js",
    "build:practice": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.practice.js",
    "practice": "npm run clear && npm run build:practice",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\" ",
    "start": "cross-env NODE_ENV=production node server/server.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "ejs": "^3.0.1",
    "koa": "^2.11.0",
    "koa-router": "^8.0.6",
    "koa-send": "^5.0.0",
    "memory-fs": "^0.5.0",
    "vue": "^2.6.11",
    "vue-meta": "^2.3.2",
    "vue-router": "^3.1.5",
    "vue-server-renderer": "^2.6.11",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "autoprefixer": "^9.7.3",
    "babel-core": "^6.26.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "concurrently": "^5.1.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^5.0.2",
